<launch>
  <arg name="model" default="$(find urdf_simplified_description)/urdf/urdf_simplified.xacro"/>
  <arg name="gui" default="true"/>
  <arg name="rvizconfig" default="$(find urdf_simplified_description)/launch/urdf.rviz"/>
  <param name="robot_description" command="$(find xacro)/xacro $(arg model)"/>
  <param name="use_gui" value="$(arg gui)"/>
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher"/>
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
  <node name="rviz_gui" pkg="rviz" type="rviz" output="screen"/>
  <node name="RSP" pkg="robot_state_publisher" type="robot_state_publisher" output="screen"/>
  <node name="JSP" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" output="screen"/>
</launch>